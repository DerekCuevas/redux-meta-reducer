{
  "name": "redux-meta-reducer",
  "version": "0.0.1",
  "description": "A redux higher order reducer to simplify the state of fetched data.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "homepage": "https://github.com/DerekCuevas/redux-meta-reducer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DerekCuevas/redux-meta-reducer.git"
  },
  "bugs": {
    "url": "https://github.com/DerekCuevas/redux-meta-reducer/issues"
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src/** test/** examples/**",
